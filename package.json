{
	"name": "focus-svelte",
	"version": "0.3.0",
	"license": "MIT",
	"author": {
		"name": "chance dinkins",
		"email": "chanceusc@gmail.com"
	},
	"repository": {
		"url": "https://github.com/chanced/focus-svelte",
		"type": "git"
	},
	"keywords": [
		"focus",
		"tabindex",
		"focus trap",
		"focus lock",
		"accessibility",
		"ada",
		"svelte",
		"sveltekit",
		"aria"
	],
	"bugs": {
		"url": "https://github.com/chanced/focus-svelte/issues"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "npm run format && npm run lint && svelte-kit package",
		"publish": "npm run package && npm publish ./package",
		"playwright:test": "npx playwright test",
		"test": "concurrently -k \"npm:dev\" \"npm:playwright:test\"",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"concurrently": "^6.2.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.43.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.5",
		"svelte2tsx": "^0.4.6",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"@playwright/test": "^1.15.0"
	},
	"type": "module"
}

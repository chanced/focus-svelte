{
	"name": "focus-svelte",
	"version": "0.4.0",
	"license": "MIT",
	"author": {
		"name": "chance dinkins",
		"email": "chanceusc@gmail.com"
	},
	"repository": {
		"url": "https://github.com/chanced/focus-svelte",
		"type": "git"
	},
	"keywords": [
		"focus",
		"tabindex",
		"focus trap",
		"focus lock",
		"accessibility",
		"ada",
		"svelte",
		"sveltekit",
		"aria"
	],
	"bugs": {
		"url": "https://github.com/chanced/focus-svelte/issues"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "npm run format && npm run lint && svelte-kit package",
		"publish": "npm run package && npm publish ./package",
		"playwright:test": "npx playwright test",
		"test": "concurrently -k \"npm:dev\" \"npm:playwright:test\"",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"exports": {
		".": {
			"svelte": "./index.js"
		}
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.5.4",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"@typescript-eslint/parser": "^7.3.1",
		"concurrently": "^6.2.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.0",
		"svelte-check": "^3.6.8",
		"svelte-preprocess": "^5.1.3",
		"svelte2tsx": "^0.7.5",
		"tslib": "^2.6.2",
		"typescript": "^5.0.0",
		"@playwright/test": "^1.15.0"
	},
	"type": "module"
}
